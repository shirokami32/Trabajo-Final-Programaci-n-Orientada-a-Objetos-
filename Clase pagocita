// Clase EspecialidadMedica
class EspecialidadMedica {
    private String nombre;

    public EspecialidadMedica(String nombre) {
        this.nombre = nombre;
    }

    public String getNombre() {
        return nombre;
    }

    @Override
    public String toString() {
        return "Especialidad: " + nombre;
    }
}
